        "def cal(m,M):\n",
        "    n = M-m+1\n",
        "    return (n*(m+M))//2\n",
        "num = list(map(int,input().split()))\n",
        "a = min(num)\n",
        "b = max(num)\n",
        "print(cal(a,b))"
