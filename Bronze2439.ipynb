        "m = n = int(input())\n",
        "i = 1\n",
        "while i<=m:\n",
        "    a=b=1\n",
        "    while a<n:\n",
        "        print(' ',end='')\n",
        "        a+=1\n",
        "    n-=1\n",
        "    while b<(i+1):\n",
        "        print('*',end='')\n",
        "        b+=1\n",
        "    print()\n",
        "    i+=1"
